{
  "name": "rspack-react-ts-starter",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently --kill-others \"nodemon -r dotenv/config ./service/index.ts\" \"rspack build -w\"",
    "build": "rspack build & tsc --project service",
    "start": "node -r dotenv/config ./service-dist/index.js"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "body-parser": "^1.20.2",
    "express": "^4.18.3",
    "moment": "^2.30.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@rspack/cli": "0.5.7",
    "@rspack/core": "0.5.7",
    "@rspack/plugin-react-refresh": "0.5.7",
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.5",
    "@types/express": "^4.17.21",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "nodemon": "^3.1.0",
    "react-refresh": "^0.14.0",
    "ts-node": "^10.9.2"
  }
}
